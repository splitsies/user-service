{
  "name": "user-service",
  "version": "1.0.8",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "clear-shared-models": "npm cache clean --force && rm -rf ./node_modules/@splitsies/shared-models && rm -rf ./package-lock.json",
    "clear-utils": "npm cache clean --force && rm -rf ./node_modules/@splitsies/utils && rm -rf ./package-lock.json && npm i",
    "serve": "sam build && sam local start-api --host 0.0.0.0 --port 6001 --env-vars ./local-startup/env.json",
    "build": "sam build"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "prettier": "@splitsies/formatting",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.658.1",
    "@aws-sdk/client-sns": "^3.658.1",
    "esbuild": "^0.14.11",
    "firebase": "^10.4.0",
    "inversify": "6.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@splitsies/formatting": "^1.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "esbuild": "^0.14.11",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^2.8.7",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^12.0.4",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
